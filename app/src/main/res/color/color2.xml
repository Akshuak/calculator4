<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" android:color="#B2FF59">
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#B2FF59" />
            <gradient android:angle="-90" android:startColor="#B2FF59" android:endColor="#B2FF59"  />
        </shape>
    </item>

    <item android:color="#B2FF59">
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#B2FF59" />
            <gradient android:angle="-90" android:startColor="#B2FF59" android:endColor="#B2FF59" />
        </shape>
    </item>
</selector>